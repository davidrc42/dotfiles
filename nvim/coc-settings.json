{
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "json",
    "typescript",
    "javascript",
    "javascriptreact",
    "typescriptreact"
  ],
  "eslint.autoFixOnSave": true,
  "eslint.filetypes": ["javascript", "javascriptreact", "typescript"],
  "prettier.disableSuccessMessage": true,
  "pairs.enableCharacters": ["(", "[", "{", "'", "\"", "`"]
}
